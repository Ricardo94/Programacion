#include "stdafx.h"
#include "math.h"
#define NMAX 100
int main (void)
{	
	int n,i,A[NMAX],s=0;
	printf("\n INGRESE LA CANTIDAD DE NUMEROS: ");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&A[i]);///****************
	}
	s=0;
	for(i=0;i<n;i++)
	{
		s=s+A[i];
	}
	printf("\n LA SUMA ES:%d",s);
return 0;
	}

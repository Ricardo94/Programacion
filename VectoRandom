#include "stdafx.h"
#include "stdlib.h"
#include <time.h>
#define NMAX 100
using namespace std;
 
int main (void)
{
	int n,A[NMAX];
srand((unsigned)time(NULL));
printf("\n Ingrese cantidad de Numeros Aleatorios: ");
scanf("%d",&n);
for(int i=1;i<=n;i++)
{
	A[i]=rand()%6+1;
}
for(int i=0;i<=n;i++)
{
	printf("\n %d",A[i]);
}
	return 0;
 } 
